{
  "DataBaseConfiguration": {
    "TimeoutInSeconds": 1000,
    "MaxTimeInSeconds": 20,
    "RetryOnFailure": {
      "Enable": true,
      "MaxTimeOutInSeconds": 30,
      "RetryCount": 5
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
